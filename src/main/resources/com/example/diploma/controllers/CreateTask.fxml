<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.diploma.controllers.CreateTaskController">
   <children>
      <TextField fx:id="nameField" layoutX="43.0" layoutY="58.0" promptText="task's name" />
      <ComboBox fx:id="projectsField" layoutX="289.0" layoutY="58.0" prefHeight="31.0" prefWidth="173.0" promptText="project's name" />
      <ComboBox fx:id="usersField" layoutX="524.0" layoutY="58.0" prefWidth="150.0" promptText="user's name" />
      <DatePicker fx:id="dateField" layoutX="43.0" layoutY="121.0" prefHeight="31.0" prefWidth="155.0" />
      <Button fx:id="createTaskButton" layoutX="314.0" layoutY="152.0" mnemonicParsing="false" onAction="#create_task" text="Create task" />
   </children>
</AnchorPane>
